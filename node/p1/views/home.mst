{{> header}}
<h1>{{titlePage}}</h1>


{{#posts}}
    <div class="post">
        {{#photo}}
        <img src="media/{{photo}}" />
        {{/photo}}
        <H3><a href="/post/{{slug}}">{{title}}</a></H3>
        <p>{{body}}</p>
        <a href="/post/{{slug}}/edit">[ Editar ]</a>
        <a href="/post/{{slug}}/delete"> [ Excluir ]</a>
        <hr/>
    </div>
{{/posts}}